{
  "name": "@quantimodo/quantimodo-sdk-typescript-fetch",
  "version": "6.0.0",
  "description": "A Typescript Fetch client to retrieve and store life-tracking data using the QuantiModo API",
  "author": "QuantiModo",
  "keywords": [
    "fetch",
    "typescript",
    "quantimodo",
    "@quantimodo/quantimodo-sdk-typescript-fetch"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc --outDir dist/",
    "lint": "npm run lint:es && npm run lint:ts && npm run lint:yaml",
    "lint:es": "eslint .",
    "lint:es-fix": "eslint . --fix",
    "lint:fix": "npm run lint:es-fix && npm run lint:ts-fix",
    "lint:ts": "cd ts && tslint --project .",
    "lint:ts-fix": "cd ts && tslint --project . --fix",
    "lint:yaml": "yamllint '*.yml' 'basic/*.yml' .buildkite/pipeline.yml .semaphore/semaphore.yml '.github/workflows/*.yml'",
    "prepublishOnly": "npm run build",
    "test": "npm run build && npm run test:mocha",
    "test:mocha:browser": "mocha -r jsdom-global/register tests/api/**/*.spec.js",
    "test:mocha:ts": "mocha -r ts-node/register tests/**/*.spec.js",
    "openapi-generator-cli": "openapi-generator generate -g typescript-fetch -i open-api.json -o ./ --additional-properties=\"putParametersInObject=true\""
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.13-4.3.1",
    "portable-fetch": "^3.0.0"
  },
  "devDependencies": {
    "@octokit/rest": "16.35.2",
    "@types/mocha": "5.2.7",
    "@types/node": "8.10.60",
    "eslint": "5.16.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-mocha": "5.3.0",
    "expect.js": "^0.3.1",
    "jsdom": "16.2.2",
    "jsdom-global": "3.0.2",
    "mocha": "6.2.2",
    "mocha-junit-reporter": "1.23.3",
    "mocha-junit-reporters": "1.23.6",
    "mocha-multi-reporters": "1.1.7",
    "mochawesome": "4.1.0",
    "mochawesome-merge": "2.1.0",
    "mochawesome-report-generator": "4.0.1",
    "ts-node": "8.5.4",
    "typescript": "2.9.2"
  },
  "publishConfig": {
    "registry": "https://skimdb.npmjs.com/registry"
  }
}
